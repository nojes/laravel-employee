{
    "name": "nojes/laravel-employees",
    "type": "package",
    "description": "This package for employees management for Laravel Framework.",
    "keywords": [
        "nojes",
        "laravel-employees",
        "laravel",
        "employees",
        "employee",
        "crud"
    ],
    "homepage": "https://github.com/nojes/laravel-employees",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vyacheslav Nozhenko",
            "email": "vv.nojenko@gmail.com",
            "homepage": "https://github.com/nojes",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "autoload": {
        "psr-4": {
            "nojes\\employees\\": "src",
            "nojes\\employees\\database\\seeds\\": "database/seeds"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nojes\\employees\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "nojes\\employees\\EmployeesServiceProvider"
            ]
        }
    }
}
